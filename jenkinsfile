pipeline {
    agent any
    stages {
        stage('hello') {
            steps {
                echo "Hello World"
            }
        }
        stage('build') {
            steps {
                echo "Building..."
            }
        }
         stage('test') {
            steps {
                echo "Testing..."
            }
        }
        stage('deploy') {
            steps {
                echo "Deploying..."
            }
        }
    }
}
